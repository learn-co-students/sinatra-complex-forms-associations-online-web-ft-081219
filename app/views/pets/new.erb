<h1>Create a new Pet</h1>

<form action="/pets" method="POST"> 
  <input type="text" name="pet_name" id="pet_name">
  <h2>Existing Owners</h2>
  <% @owners.each do |owner| %>  
   <input type="radio" value="<%= owner.id %>" name="pet[owner_id]" id="<%=owner.id %>" > <%= owner.name %>  <br> 
   <%= owner.name %>
  <% end %>
   <br>
  <input id="owner_name" name="owner_name" type="text">
  <button type="submit" id="submit">Create Pet</button>
</form>